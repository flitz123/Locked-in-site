{
    "version": 2,
    "builds": [
        {
            "src": "api/download.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/download",
            "dest": "/api/download.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}